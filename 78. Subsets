#recursive
class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
        res = []
        self.helper(nums, 0, [], res)
        return res
    
    def helper(self, nums, index, curr, res):
        res.append(curr)
        for i in range(index, len(nums)):
            self.helper(nums, i+1, curr+[nums[i]], res)
          
#iterative
class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
        res = [[]]
        for num in nums:
            res += [item + [num] for item in res]
        return res
        
            
